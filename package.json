    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^29.5.0",
    "nodemon": "^1.4.1",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.3.0",
    "@okta/jwt-verifier": "^3.0.1",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "custom-env": "^2.0.1",
    "ejs": "^3.0.2",
    "express": "^4.18.2",
    "express-paginate": "^1.0.2",
    "express-validator": "^6.14.2",
    "http": "^0.0.1-security",
    "http-status": "^1.5.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^12.4.0",
    "mongoose": "^6.4.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.8.0",
    "pg": "^8.7.3",
    "pick": "^0.0.1",
    "save": "^2.4.0",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.6.0",
    "util": "^0.12.4",
    "uuid": "^9.0.1",
    "validate": "^5.2.0",
    "validator": "^13.7.0",
    "websocket": "^1.0.34",
    "winston": "^3.5.1",
    "winston-daily-rotate-file": "^4.6.1",
    "xlsx": "^0.18.5"
  }
