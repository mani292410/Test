{
  "name": "@mineral/cra-starter",
  "version": "0.1.0",
  "private": true,
  "main": "src/server.js",
  "engines": {
    "node": ">=16.13.2"
  },
  "scripts": {
    "systemd": "/home/fernandus/.nvm/versions/node/v16.15.0/bin/node  src/server.js",
    "seed:windows:local": "export NODE_ENV=localhost&& npx sequelize-cli db:seed:all",
    "migrate:windows:db1": "export NODE_ENV=localhost&& export NODE_MIGRATION_ENV=db1&& npx sequelize-cli db:migrate",
    "migrate:windows:db2": "export NODE_ENV=localhost&& export NODE_MIGRATION_ENV=db2&& npx sequelize-cli db:migrate",
    "migrate:windows:dev": "SET NODE_ENV=dev&& npx sequelize-cli db:migrate",
    "migrate:linux:dev": "export NODE_ENV=dev&& npx sequelize-cli db:migrate",
    "migrate:linux:dev:db1": "export NODE_ENV=dev&& export NODE_MIGRATION_ENV=db1&& npx sequelize-cli db:migrate",
    "migrate:linux:dev:db2": "export NODE_ENV=dev&& export NODE_MIGRATION_ENV=db2&& npx sequelize-cli db:migrate",
    "migrate:windows:prod": "SET NODE_ENV=prod&& npx sequelize-cli db:migrate",
    "migrate:linux:prod": "export NODE_ENV=prod&& npx sequelize-cli db:migrate",
    "start:windows:local": "export NODE_ENV=localhost&& nodemon src/server.js",
    "start:windows:dev": "SET NODE_ENV=dev&& pm2 start src/server.js",
    "start:windows:prod": "SET NODE_ENV=prod&& pm2 start src/server.js",
    "start:linux:local": "export NODE_ENV=localhost&& nodemon src/server.js",
    "start:linux:dev": "export NODE_ENV=dev&& pm2 start src/server.js",
    "start:linux:prod": "export NODE_ENV=prod&& pm2 start src/server.js",
    "start:linux:docker:dev": "export NODE_ENV=dev&& pm2-runtime start src/server.js",
    "start:linux:docker:prod": "export NODE_ENV=prod&& pm2-runtime start src/server.js",
    "start": "nodemon src/server.js NODE_ENV=dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "export NODE_ENV=dev && jest --watchAll=false --forceExit"
  },
  "jest": {
    "collectCoverage": true
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "nodejs",
    "rest",
    "api",
    "sequelize",
    "mysql"
  ],
  "devDependencies": {
    "eslint": "^8.13.0",
